{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:start-d": "docker compose -f ./docker/dev-docker-compose.yml --compatibility --env-file ./docker/dev.env up --remove-orphans --build -d",
    "dev:start": "docker compose -f ./docker/dev-docker-compose.yml --compatibility --env-file ./docker/dev.env up --remove-orphans --build",
    "dev:start-wb": "docker compose -f ./docker/dev-docker-compose.yml --compatibility --env-file ./docker/dev.env up --remove-orphans",
    "dev:rm": "docker compose -f ./docker/dev-docker-compose.yml --compatibility --env-file ./docker/dev.env rm",
    "dev:stop": "docker compose -f ./docker/dev-docker-compose.yml --compatibility --env-file ./docker/dev.env down",
    "dev:restart": "npm run dev:stop && npm run dev:start",
    "prod:start": "docker compose -f ./docker/prod-docker-compose.yml --compatibility --env-file ./docker/prod.env up --remove-orphans --build",
    "prod:restart": "npm run prod:stop && npm run prod:start",
    "prod:stop": "docker compose -f ./docker/prod-docker-compose.yml --compatibility --env-file ./docker/prod.env down",
    "test-p": "npx playwright test",
    "test-j": "jest --coverage -u",
    "test-j:watch": "jest --watch"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "dexie": "^4.0.8",
    "hono": "^4.7.4",
    "jose": "^5.10.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.9.0",
    "mongoose": "^8.7.1",
    "next": "^15.0.3",
    "next-logger": "^5.0.1",
    "node-fetch": "^2.7.0",
    "node-mocks-http": "^1.16.2",
    "pino": "^9.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-player": "^2.16.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.1.2",
    "@playwright/test": "^1.48.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/jest-json-schema": "^6.1.4",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "framer-motion": "^11.5.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-json-schema": "^6.1.0",
    "monocart-coverage-reports": "^2.11.2",
    "nodemon": "^3.1.7",
    "postcss": "^8",
    "sass": "^1.79.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
